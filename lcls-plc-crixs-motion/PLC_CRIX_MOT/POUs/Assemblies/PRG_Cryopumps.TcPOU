                                             fDelta:=1.0,
                                             fVelocity:=5.0,
                                             fAccel:=1.0,
                                            fDecel:=1.0,
                                             bMoveOk:=FALSE,
                                             bLocked:=TRUE,
                                             bValid:=TRUE);

    // Y axis position states
    stCP2PosPark : DUT_PositionState :=		(sName:= 'PARK',
                                             fPosition := 0.0,
                                             fDelta:=1.0,
                                             fVelocity:=5.0,
                                             fAccel:=1.0,
                                            fDecel:=1.0,
                                             bMoveOk:=FALSE,
                                             bLocked:=TRUE,
                                             bValid:=TRUE);
END_VAR
]]></Declaration>
